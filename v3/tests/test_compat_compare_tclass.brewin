(tclass bruh (p)
    (field p x)
    (method p get_x () (return x))
    (method void set_x ((p y)) (set x y))
)

(class main
    (method void main ()
        (let ((bruh@int x) (bruh@int y))
            (print (== x y))
            (set x (new bruh@int))
            (set y (new bruh@int))
            (call x set_x 7)
            (call y set_x 7)
            (print (== (call x get_x) (call y get_x)))
            (call y set_x 5)
            (print (== (call x get_x) (call y get_x)))
        )
    )
)