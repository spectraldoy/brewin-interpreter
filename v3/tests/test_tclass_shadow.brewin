(tclass bruh (p)
    (field p x)
    (method p get_x () (return x))
    (method void set_x ((p y)) (set x y))
    (method void print_something ((int p)) (print p))
)

(class main
    (method void main ()
        (let ((bruh@int x))
            (set x (new bruh@int))
            (call x print_something 5)
        )
    )
)